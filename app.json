{
  "expo": {
    "name": "U-CHAT",
    "slug": "U-CHAT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6f4e37"
    },
    "updates": {
      "fallbackToCacheTimeout": 60000,
      "url": "https://u.expo.dev/5131dff2-533a-423a-b093-a6c141ec4061"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6f4e37"
      },
      "package": "com.saketkumar.UCHAT"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#6f4e37"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#6f4e37"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#6f4e37",
          "sounds": [
            "./assets/tones/pushtone.mp3"
          
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5131dff2-533a-423a-b093-a6c141ec4061"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
